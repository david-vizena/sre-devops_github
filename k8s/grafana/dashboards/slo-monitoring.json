{
  "id": null,
  "uid": "slo-monitoring",
  "title": "SLO Monitoring",
  "tags": [
    "slo",
    "portfolio",
    "operations"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "style": "dark",
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "refresh": 1,
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "Prometheus"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Availability SLO",
      "description": "Current availability percentage (Target: 99.9%)",
      "datasource": "$datasource",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.99 },
              { "color": "green", "value": 0.999 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "avg(service_up)",
          "legendFormat": "Availability",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "description": "Current error rate (Target: < 0.1%)",
      "datasource": "$datasource",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 0.01,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.001 },
              { "color": "red", "value": 0.005 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "1 - avg(service_up)",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "type": "stat",
      "title": "P95 Latency",
      "description": "95th percentile latency (Target: < 500ms)",
      "datasource": "$datasource",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "decimals": 3
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "http_request_duration_seconds{quantile=\"0.95\"}",
          "legendFormat": "P95 Latency",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "type": "stat",
      "title": "Request Rate",
      "description": "Current requests per second (Target: 1000 req/s)",
      "datasource": "$datasource",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "Request Rate",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Availability Over Time",
      "description": "Service availability percentage (Target: 99.9%)",
      "datasource": "$datasource",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.99 },
              { "color": "green", "value": 0.999 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "avg(service_up)",
          "legendFormat": "Availability",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Latency Percentiles",
      "description": "Request latency percentiles (Target: P95 < 500ms)",
      "datasource": "$datasource",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "decimals": 3
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "http_request_duration_seconds{quantile=\"0.5\"}",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "http_request_duration_seconds{quantile=\"0.95\"}",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "http_request_duration_seconds{quantile=\"0.99\"}",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate Over Time",
      "description": "Error rate percentage (Target: < 0.1%)",
      "datasource": "$datasource",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 0.01,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.001 },
              { "color": "red", "value": 0.005 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "1 - avg(service_up)",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate Over Time",
      "description": "Requests per second (Target: 1000 req/s)",
      "datasource": "$datasource",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "Request Rate",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "type": "table",
      "title": "SLO Status by Service",
      "description": "Current SLO metrics per service",
      "datasource": "$datasource",
      "gridPos": { "x": 0, "y": 20, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "service_up",
          "legendFormat": "{{service}}",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "app": true,
              "instance": true,
              "job": true
            },
            "indexByName": {
              "Value": 2,
              "service": 1
            },
            "renameByName": {
              "Value": "Status (1=Up)",
              "service": "Service"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      }
    }
  ]
}

